	<!-- The animation duration in milliseconds -->
	<AnimationDuration>1200</AnimationDuration>
	
	<!-- The number of frames in an animation sequence -->
	<AnimationFrames>12</AnimationFrames>
	
	<!-- Width and height of one single animation frame in pixel -->
